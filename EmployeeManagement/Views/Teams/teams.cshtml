@model System.Data.DataTable
@{
    ViewBag.Title = "teams";
}
@Styles.Render("~/Content/Table.css")
<h2>Teams</h2>

<table class="tables">
    <tr>
        <th>TeamId</th>
        <th>TeamName</th>
        <th>Manager</th>
        <th></th>
    </tr>
    @{ 
        for(int i=0; i< Model.Rows.Count; i++)
            {
        <tr>
            <td>@Model.Rows[i][0]</td>
            <td>@Model.Rows[i][1]</td>
            <td>@Model.Rows[i][2]</td>
            <td><a href="@Url.Action("Viewmembers", "Teams", new { @id=Model.Rows[i][0] })">View Team</a></td>
        </tr>
               
            }
    }
</table>



